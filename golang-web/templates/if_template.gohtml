<html>
    <!-- 
        eq	artinya arg1 == arg2
        ne	artinya arg1 != arg2
        lt	artinya arg1 < arg2
        le	artinya arg1 <= arg2
        gt	artinya arg1 > arg2
        ge	artinya arg1 >= arg2
     -->
    <body>
        {{if.Name}}
        <h1>Hello {{.Name}}</h1>
        {{else}}
        <h1>Hello</h1>
        {{end}}
        {{if ge .FinalValue 80}}
        <h1>Good</h1>
        {{else if ge .FinalValue 80}}
        <h1>Jos Tenan</h1>
        {{else}}
        <h1>Try Again</h1>
        {{end}}
    </body>
</html>